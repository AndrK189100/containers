{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-new": "off",
    "no-console": "off",
    "import/extensions": ["off"]
    
  }

}